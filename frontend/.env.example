# Frontend Environment Variables
# Copy this file to .env and fill in your actual API keys

# OpenRouter API Key for LLM access
# Get your API key from: https://openrouter.ai/keys
# This should be the same as the backend OPENROUTER_API_KEY
OPENROUTER_API_KEY=your-openrouter-key

# OpenAI API Key (required by CopilotKit)
# Must be set to the same value as OPENROUTER_API_KEY
# CopilotKit's OpenAIAdapter checks this environment variable internally
# The code configures it to use OpenRouter's API endpoint via baseURL option
OPENAI_API_KEY=your-openrouter-key

# Backend API URL
# Points to the FastAPI backend endpoint
# Default backend runs on: http://127.0.0.1:8006/crewai-agent
NEXT_PUBLIC_CREWAI_URL=http://127.0.0.1:8006/crewai-agent

# Frontend Development Server Port (default: 3006)
# Configured in package.json: "dev": "next dev --turbopack -p 3006"
# Frontend URL: http://localhost:3006
